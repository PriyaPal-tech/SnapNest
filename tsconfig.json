{
    "compilerOptions": {
        "target": "es2015",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "paths": {
            "@/types": [
                "./src/types"
            ],
            "@/types/*": [
                "./src/types/*"
            ],
            "@/constants": [
                "./src/constants"
            ],
            "@/constants/*": [
                "./src/constants/*"
            ],
            "@/enums": [
                "./src/enums"
            ],
            "@/enums/*": [
                "./src/enums/*"
            ],
            "@/requests/*": [
                "./src/requests/*"
            ]
        }
    },
    "include": [
        "src/**/*"
    ]
}